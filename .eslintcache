\n    }\r\n\r\n    //recuperer les qrcodes appartenant à un etablissement\r\n    const getAllQRLieu = () => {\r\n        return qrService.getAllQRLieu(localStorage.getItem(\"token\"))\r\n        .then(response => {return response})\r\n\r\n    }\r\n    //s'inscrire \r\n    const sInscrire = (inscription,type) => {\r\n        return qrService.sInscrire(inscription,type)\r\n        .then(response=>{\r\n            if(response.status === 200){\r\n                toutEnregistrer(response.data,type)\r\n            }\r\n            return response\r\n        })\r\n    }\r\n    //se connecter\r\n    const seConnecter = (connexion,type) => {\r\n        return qrService.seConnecter(connexion,type)\r\n        .then(response => {\r\n            if(response.status === 200){\r\n                toutEnregistrer(response.data,type)   \r\n            }\r\n            return response\r\n        })\r\n    }\r\n\r\n    //se deconnecter\r\n    const seDeconnecter= () => {\r\n        setListQRCode([])\r\n        localStorage.removeItem(\"token\")\r\n        localStorage.removeItem(\"nom\")\r\n        localStorage.removeItem(\"type\")\r\n        setToken()\r\n        return true\r\n    }\r\n\r\n    //creer un qrcode pour un lieu \r\n    const creerQRCodeLieu = (qrcode) => {\r\n        var data = {\r\n            token : localStorage.getItem(\"token\"),\r\n            data : qrcode\r\n        }\r\n        return qrService.creerQRCodeLieu(data)\r\n        .then(getAllQRLieu())\r\n        .catch(error=>{\r\n            console.log(\"erreur pour créer un nouveau QRCODE \",error)\r\n        })\r\n    }\r\n\r\n    //Creer nombre qrcode pour un malade\r\n    const creerQRCodeMedecin = (nombre) => {\r\n        const data = {token : localStorage.getItem(\"token\")}\r\n        var liste = []\r\n        for (let i = 0; i < nombre; i++) {\r\n            const qrcode = nombre//qrService.creerQRCodeMedecin(data)\r\n            liste = liste.concat(qrcode)\r\n        }\r\n        console.log(liste)\r\n        return liste\r\n    }\r\n    \r\n\r\n    const exposeValue = {\r\n        listeQRCode,\r\n        getAllQRLieu,\r\n        setListQRCode,\r\n        seConnecter,\r\n        sInscrire,\r\n        seDeconnecter,\r\n        creerQRCodeLieu,\r\n        creerQRCodeMedecin\r\n        \r\n    }\r\n\r\n    return(\r\n        <Context.Provider value={exposeValue}>\r\n            {props.children}\r\n        </Context.Provider>\r\n    )\r\n}\r\nexport {\r\n    Context,\r\n    ProviderWrapper\r\n}\r\nexport default Context","D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\services\\CovidBlockServices.js",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\Connexion\\Connexion.jsx",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\Inscription\\Inscription.jsx",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\Header\\Header.jsx",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\QRCodeLieu\\QRCodeLieu.jsx",[],["86","87"],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\Accueil\\Accueil.jsx",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\ListeQRCode\\ListeQRCodeLieu.jsx",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\Etablissement\\Etablissement.jsx",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\Medecin\\Medecin.jsx",[],"D:\\Documents\\OneDrive\\Documents\\IPL 2020-2021\\PFE\\WorkSpace\\Front Web\\blockcovid-front-web\\src\\components\\CreerQR\\CreerQRLieu